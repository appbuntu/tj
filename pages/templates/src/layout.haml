!!!
%html
  %head
    %meta(http-equiv="content-type" value="text/html;charset=utf8")
    %meta(name="generator" value="{{generator}}")
    %title {{title}}
    {{{stylesheet_tags}}}
  %body

    %header.header__wrapper
      .header
        %aside.logo
          %img.logo__image(src="" alt="Theme Juice")
        %nav.navigation
          {{#section_heads}}
          %a.navigation__item(href="\#{{id}}" alt="{{text}}")
            {{text}}
          {{/section_heads}}

    %main.main__wrapper
      .main

        %ol.main__head
          %li.main__head__item
            {{page_name}}
          %li.main__head__item
            {{manual}}
          %li.main__head__item
            {{page_name}}

        %article.main__content
          {{{html}}}

        %ol.main__foot
          %li.main__foot__item
            {{organization}}
          %li.main__foot__item
            {{date}}
          %li.main__foot__item
            {{page_name}}
